import sys
import math

ans = [ float(formula) for formula in sys.stdin ]
ans=math.prod(ans)
if ans.is_integer():
    ans=int(ans)

file=open('ans_log.txt','a')
file.write('times-> ')
file.write(str(ans)+'\n')
file.write('----------'+'\n')
file.close()
print(ans)
